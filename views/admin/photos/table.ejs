<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < photos.length; i++){ %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= photos[i].name %></td>
      <td>
        <form
          action="/photos/delete/<%= photos[i]._id %>?_method=DELETE"
          method="post"
        >
          <a
            href="/photos/sub-photos/<%= photos[i]._id %>"
            class="btn btn-info btn-sm"
          >
            <i class="fas fa-pencil-alt"></i>
            Tambah Photos
          </a>
          <a
            href="/photos/edit/<%= photos[i]._id %>"
            class="btn btn-info btn-sm"
          >
            <i class="fas fa-pencil-alt"></i>
            Ubah
          </a>
          <button class="btn btn-danger btn-sm">
            <i class="fas fa-trash-alt"></i>
            Hapus
          </button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
