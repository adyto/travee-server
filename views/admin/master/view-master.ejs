    <div class="wrapper">
      <div class="content-wrapper">
        <% if(role == "admin") { %>
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <div class="container-fluid">
            <%- include ../../partials/message.ejs %>
            <div class="row mb-2">
              <div class="col-sm-6">
                <h1>Master</h1>
              </div>
              <div class="col-sm-6">
